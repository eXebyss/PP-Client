datasource db {
    provider = "mongodb"
    url      = env("MONGODB_DATABASE_URL")
}

generator client {
    provider      = "prisma-client-js"
}

model messages {
    id         String    @id @default(auto()) @map("_id") @db.ObjectId
    name       String?
    email      String
    message    String?
    date       DateTime? @default(now())
    dateString String?
}
