{
	"trailingComma": "es5",
	"endOfLine": "lf",
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"importOrder": [
		"^(next/(.*)$)|^(next$)",
		"<THIRD_PARTY_MODULES>",
		"next-seo.config",
		"^components/(.*)$",
		"^utils/(.*)$",
		"^assets/(.*)$",
		"^@fontsource/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
